#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int main () {

    std::vector<int> ids = {1,2,3,4,5};

    // while loop example
    bool condition = false;
    while (condition == false) {
        cout << "This will never be printed." << endl;
    }


    // for loop example
    for (int i = 0; i < 5; i++) {
        cout << "This will be printed 5 times." << endl;
    }

    // if-else example
    int age = 20;
    if (age < 18) {
        cout << "Access DENIED." << endl;
    } else {
        cout << "Access GRANTED." << endl;
    }

    return 0;
}
